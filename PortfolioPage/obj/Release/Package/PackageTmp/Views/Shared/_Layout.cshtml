<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - AJ Sigmon Portfolio</title>
    <!-- Default Links -->
    @*@Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")*@ 
    @*@Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")*@
    <link href="~/Content/PagedList.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/owl.carousel.css" />
    <link rel="stylesheet" href="~/css/magnific-popup.css" />
    <link rel="stylesheet" href="~/css/font-awesome.css" />
    <link rel="stylesheet" href="~/css/responsive.css" />
    <link rel="stylesheet" href="~/css/style.css" />
    <!-- Favicon -->
    <link rel="shortcut icon" href="~/images/icon/favicon.png">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/images/icon/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/images/icon/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/images/icon/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/images/icon/apple-touch-icon-57-precomposed.png">
    <!-- My CSS -->
    <link href="~/css/prism.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/AJStyles.css" />
</head>
<body>
    <header id="header">
        <nav class="navbar st-navbar navbar-fixed-top nav-ajcontainer">
            <div class="container">
                <div class="row">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#st-navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <div class="col-xs-10"><a href="@Url.Action("index","Home")#header"><h3 class="toplogo">Sigmon</h3></a></div>
                    </div>

                    <div class="collapse navbar-collapse" id="st-navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li><a href="@Url.Action("index","Home")#skills-section">Skills</a></li>
                            <li><a href="@Url.Action("Resume","Home")#resume-section">Resume</a></li>
                            <li><a href="@Url.Action("Projects","Home")#project-section">Projects</a></li>
                            <li><a href="@Url.Action("index", "BlogPosts")">Blog</a></li>
                            <li><a href="@Url.Action("index","Home")#contact">Contact</a></li>
                            <li role="separator" class="divider"></li>
                        </ul>
                        <div class="loginlinks">@Html.Partial("_LoginPartial")</div><!--Hidden for Moble Responsiveness-->
                    </div><!-- /.navbar-collapse -->
                </div><!-- /.container row -->
                <div class="row nav-ajlogin">
                    <div class="nav-triangle"></div>
                    @Html.Partial("_LoginPartial")<!--Shown until Moble size for Responsiveness-->
                </div>
            </div><!-- /.container -->
        </nav>
    </header>

    @RenderBody()

    <footer class="footer" id="footer">
        <div class="container">
            <div class="row">
                <div class="col-xs-6 col-sm-6 col-sm-6 copyright">
                    <p>&copy; 2016 <a href="">Sigmonster Enterprises</a>. All Rights Reserved.</p>
                </div>
                <!-- SOCIAL ICONS -->
                <div class="col-xs-6 col-sm-6 col-sm-6 footer-social-icons">
                    <div class="row">
                        <span>Follow Me:</span>
                        <a href="https://github.com/sigmonster"><i class="fa fa-github"></i></a>
                        <a href="https://www.linkedin.com/in/alexandersigmon"><i class="fa fa-linkedin"></i></a>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <script type="text/javascript" src="~/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/js/jquery.parallax.js"></script>
    <script type="text/javascript" src="~/js/smoothscroll.js"></script>
    <script type="text/javascript" src="~/js/masonry.pkgd.min.js"></script>
    <script type="text/javascript" src="~/js/jquery.fitvids.js"></script>
    <script type="text/javascript" src="~/js/owl.carousel.min.js"></script>
    <script type="text/javascript" src="~/js/jquery.counterup.min.js"></script>
    <script type="text/javascript" src="~/js/waypoints.min.js"></script>
    <script type="text/javascript" src="~/js/jquery.isotope.min.js"></script>
    <script type="text/javascript" src="~/js/jquery.magnific-popup.min.js"></script>
    <script type="text/javascript" src="~/js/scripts.js"></script>
    <script type="text/javascript" src="~/js/myscripts.js"></script>
    <!--My Javascript Projects(in modals)-->
    <script type="text/javascript" src="~/CoderFoundryCodingExercises/FizzBuzzExercise/js/appFizzBuzz.js"></script>
    <script type="text/javascript" src="~/CoderFoundryCodingExercises/FactorialExercise/js/appFactor.js"></script>
    <script type="text/javascript" src="~/CoderFoundryCodingExercises/NumbersExercise/js/appNumbersExercise.js"></script>
    <script type="text/javascript" src="~/CoderFoundryCodingExercises/PalindromeExercise/js/appPalindrone.js"></script>
    <!--My Javascript-->
    <script src="~/js/prism.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
