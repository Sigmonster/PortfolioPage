                            <li><a href="@Url.Action("index", "BlogPosts")">Blog</a></li>
                            <li><a href="@Url.Action("index","Home")#contact">Contact</a></li>
                            <li role="separator" class="divider"></li>
                        </ul>
                        <div class="loginlinks">@Html.Partial("_LoginPartial")</div>
                          @*Antonio said to copy this to our new layout. Put it after the NavBar </ul> line.*@
                    </div><!-- /.navbar-collapse -->
                </div><!-- /.container row -->
                <div class="row nav-ajlogin">
                    <div class="nav-triangle"></div>
                    @Html.Partial("_LoginPartial")
                </div>
            </div><!-- /.container -->
